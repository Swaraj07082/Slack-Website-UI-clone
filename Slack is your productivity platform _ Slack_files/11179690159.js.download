var hidePopup = function() {
  document.querySelector('.optly-modal').style.display = "none";
};

// Option that user selected on survey.
var optlySelection = null;

// Options that user selected on survey.
var optlySelections = null;


// Number of options on the survey.
var numOptions = null;

var logEvent = function(eventName){
  window['optimizely'] = window['optimizely'] || [];

	if (window.TD && window.TD.clog) {
    switch(eventName) {
      case 'survey-displayed':
        TD.clog.track("WEBSITE_IMPRESSION", {
          contexts: {
            ui_context: {
             step: widget.step_name,
             step_variant: widget.variant,
             action: "impression",
             ui_component: "opt_modal",
            }
          }
        });
        
        window['optimizely'].push({
          type: "event",
          eventName: "modal_seen",
          tags: {
             "step": widget.step_name,
             "step_variant" : widget.variant,
             "ui_component": "opt_modal",
          }
        });
        break;
      case 'selection-clicked':
       TD.clog.track("WEBSITE_CLICK", {
          contexts: {
            ui_context: {
             step: widget.step_name,
             step_variant: widget.variant,
             action: "select",
             ui_component: "opt_modal",
             ui_element: "radio",
             ui_properties: {
               element_type: "radio",
               element_name: "opt_survey_item",
               element_value: optlySelection
             }
            }
          }
        });
        
        window['optimizely'].push({
          type: "event",
          eventName: 'selection_' + optlySelection,
          tags: {
             "step": widget.step_name,
             "step_variant" : widget.variant,
             "ui_component": "opt_modal",
             "ui_element": "radio",
             "selected_value": optlySelection
          }
        });
        break;
      case 'selection-submitted':
        TD.clog.track("WEBSITE_CLICK", {
          contexts: {
            ui_context: {
             step: widget.step_name,
             step_variant: widget.variant,
             action: "click",
             ui_component: "opt_modal",
             ui_element: "btn_form_submit",
             ui_properties: {
               element_name: "opt_submission_button",
               element_value: optlySelections
             }
            }
          }
        });
        
        window['optimizely'].push({
          type: "event",
          eventName: "submit",
          tags: {
             "step": widget.step_name,
             "step_variant" : widget.variant,
             "ui_component": "opt_modal",
             "ui_element": "button",
             "selected_value": optlySelections,
          }
        });
        break;
      case 'x-clicked':
       TD.clog.track("WEBSITE_CLICK", {
          contexts: {
            ui_context: {
             step: widget.step_name,
             step_variant: widget.variant,
             action: "click",
             ui_component: "opt_modal",
             ui_element: "btn_modal_close",
             ui_properties: {
               element_type: "x",
               element_name: "opt_x_link"
             }
            }
          }
        });
        
        window['optimizely'].push({
          type: "event",
          eventName: "x_clicked",
          tags: {
             "step": widget.step_name,
             "step_variant" : widget.variant,
             "ui_component": "opt_modal",
             "ui_element": "x",
          }
        });
        break;
        default:
          return;
        }
  	}
};

var getSelections = function() {
  var checkBoxSelections = Array.prototype.map.call(document.querySelectorAll('input[name=questions_list]:checked'), function(node) { return node.id; });
  
  var inputValue = document.getElementById('explanation-input').value;
  
  if (inputValue.length) {
    optlySelections = checkBoxSelections.join(',') + "," + inputValue;
    return;
  }
  
  optlySelections = checkBoxSelections.join(',');
};
 
var handleClick = function(e) {
  e.stopPropagation();
  var el = e.target;
	
	// checkbox clicked
  if ((el.nodeName === "INPUT" && el.id !== "explanation-input")) {
    optlySelection = el.id;
    
    // Enable/disabled input if user selects/deselects the last option ("Something else");
    if (el.id == numOptions) {
      var input = document.getElementById("explanation-input");
      input.disabled = !input.disabled;
    }

    logEvent("selection-clicked");

  // x button clicked
  } else if (el.classList.contains("optly-modal-close") || el.parentNode.classList.contains("optly-modal-close")) {

         hidePopup();
    		 logEvent("x-clicked");

    	// submit button clicked and selection made
      } else if (el.classList.contains("c-button--optly") && optlySelection) {
				getSelections();
        showThankYouMessage();
  			logEvent("selection-submitted");
      }
};

var showThankYouMessage = function() {
   // hide selection list 
   document.querySelector('.optly-modal-options').style.display = "none";

   // show success message
   document.querySelector(".optly-submission-message").classList.add("visible");

  // hide success message
  window.setTimeout(function(){
    hidePopup();
  }, widget.timer*1000);
};
      

var showPopup = function() {
	document.querySelector('.optly-modal').style.display = "block";
  logEvent("survey-displayed");
};

// function to fetch cookie value
var getCookie = function(name) {
 var match = document.cookie.match(name+'=([^;]*)');
 return match ? match[1] : undefined;
};

// randomize options on page load
var scrambleOptions = function() {
  var form = document.querySelector('.questions-list');
	for (var i = form.children.length; i >= 0; i--) {
    form.appendChild(form.children[Math.random() * i | 0]);
	}
};

utils.waitForElement('body').then(function(element) {
  if (window.readCookie && window.readCookie("optly_modal_seen")) return;

  var html = widget.$html;
  element.insertAdjacentHTML('beforeend', html);
	
  if (widget.should_randomize_options === "true") {
      scrambleOptions();
  }

  if (window.createCookie) {
    window.createCookie('optly_modal_seen', 1, 3, '.slack.com');
  }

  document.querySelector('.optly-modal').addEventListener('click', handleClick);
  
  numOptions = document.querySelectorAll(".checkbox-item").length;
 
	setTimeout(function(){
    showPopup();
  }, widget.timer*1000);
});
